<div class="container">
  {{#each usersDto}}
    <div class="userCard">
      <p>
        Usuario:
        {{this.first_name}}
        {{this.last_name}}
      </p>
      <p>Email: {{this.email}}</p>
      <p>Rol: {{this.rol}}</p>
    </div>
    <div>
      <button class="cambiarRol" data-user-id="{{this._id}}">Cambiar Rol</button>
    </div>
    <div>
      <button class="eliminarUsuario" data-user-id="{{this._id}}">Eliminar usuario</button>
    </div>
  {{/each}}
</div>


<link rel="stylesheet" href="/css/users.css" />
<script src="/js/user.js"></script>